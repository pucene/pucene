{
    "name": "pucene/dbal-driver",
    "description": "Dbal driver for pucene.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pucene\\DbalDriver\\": ""
        },
        "exclude-from-classmap": [
            "Tests/",
            "examples/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pucene\\DbalDriver\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes@sulu.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "scripts": {
        "test": "@phpunit Tests",
        "phpunit": "vendor/bin/phpunit"
    }
}
