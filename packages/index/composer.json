{
    "name": "pucene/index",
    "description": "Index package for pucene.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pucene\\Index\\": ""
        },
        "exclude-from-classmap": [
            "Tests/",
            "examples/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pucene\\Index\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes@sulu.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "pucene/analysis": "^0.1@dev",
        "schranz-search/seal": "^0.1@dev",
        "schranz-search/seal-memory-adapter": "^0.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "scripts": {
        "test": "@phpunit Tests",
        "phpunit": "vendor/bin/phpunit"
    }
}
