{
    "name": "pucene/search",
    "description": "Search package for pucene.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pucene\\Search\\": ""
        },
        "exclude-from-classmap": [
            "Tests/",
            "examples/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pucene\\Search\\Testing\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes@sulu.io"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/console": "^6.2"
    },
    "scripts": {
        "test": "@phpunit Tests",
        "phpunit": "vendor/bin/phpunit"
    }
}
